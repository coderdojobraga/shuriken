{
  "name": "shuriken-app",
  "version": "0.2.0-dev",
  "private": true,
  "scripts": {
    "dev": "npm-run-all develop",
    "develop": "npm-run-all less dev-server",
    "dev-server": "next dev",
    "build": "npm-run-all less make-build",
    "make-build": "next build",
    "start": "next start",
    "less": "lessc --js styles/antd-theme.less styles/antd-theme.css",
    "lint": "npm-run-all lint:*",
    "lint:js": "next lint",
    "lint:css": "stylelint --fix './**/*.{css,js,jsx,tsx}' --ignore-path .gitignore",
    "fmt": "npm-run-all format",
    "format": "prettier --write . --ignore-path .gitignore",
    "test": "npm-run-all test:*",
    "test:lint": "npm-run-all test:lint:*",
    "test:lint:js": "next lint",
    "test:lint:css": "stylelint './**/*.{css,js,jsx,tsx}' --ignore-path .gitignore",
    "test:format": "prettier --check . --ignore-path .gitignore"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.4",
    "antd": "^4.16.13",
    "less": "^4.1.1",
    "next": "^12.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.23.2",
    "redaxios": "^0.4.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.20",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.5.0",
    "stylelint-prettier": "^1.2.0",
    "typescript": "^4.4.2"
  }
}
