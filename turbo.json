{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {},
    "lint:check": {},
    "dev": {
      "cache": false,
      "outputs": [
        "dist/**",
        "build/**"
      ]
    }
  }
}
