{
  "name": "shuriken",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "develop": "npm-run-all less start-dev-server",
    "start-dev-server": "next dev",
    "build": "npm-run-all less start-build",
    "start-build": "next build",
    "start": "next start",
    "less": "lessc --js styles/antd-theme.less styles/antd-theme.css",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint --fix . --ignore-path .gitignore",
    "lint:css": "stylelint --fix './**/*.{css,js}' --ignore-path .gitignore",
    "fmt": "npm-run-all format",
    "format": "prettier --write . --ignore-path .gitignore",
    "test": "npm-run-all test:*",
    "test:lint": "npm-run-all test:lint:*",
    "test:lint:js": "eslint . --ignore-path .gitignore",
    "test:lint:css": "stylelint './**/*.{css,js}' --ignore-path .gitignore",
    "test:format": "prettier --check . --ignore-path .gitignore"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.3",
    "antd": "^4.16.12",
    "less": "^4.1.1",
    "next": "^11.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redaxios": "^0.4.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^11.1.0",
    "autoprefixer": "^10.3.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "prettier": "^2.3.1",
    "stylelint": "^13.13.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-css-modules": "^1.5.0",
    "stylelint-config-prettier": "^7.0.0",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-tailwindcss": "0.0.1",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.20.1",
    "tailwindcss": "^2.2.7"
  }
}
